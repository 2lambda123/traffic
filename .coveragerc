[run]
branch = True
include = */traffic/*
omit = *tests*, *plugins*, *drawing*

[report]
exclude_lines =
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    def __repr__
    def plot
    def plot_time
    def encode
    def geoencode
    def _repr_html_
    def _info_html
ignore_errors = True
